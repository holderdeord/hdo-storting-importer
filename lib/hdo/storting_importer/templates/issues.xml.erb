<?xml version="1.0" encoding="UTF-8"?>
<issues>
  <%- issues.each do |issue| -%>
  <issue>
    <externalId><%= issue[:externalId] %></externalId>
    <summary><%= issue[:summary] %></summary>
    <description><%= issue[:description] %></description>
    <type><%= issue[:type] %></type>
    <status><%= issue[:status] %></status>
    <lastUpdate><%= issue[:lastUpdate] %></lastUpdate>
    <reference><%= issue[:reference] %></reference>
    <documentGroup><%= issue[:documentGroup] %></documentGroup>
    <%- if issue[:committee] -%>
    <committee><%= issue[:committee] %></committee>
    <%- end -%>
    <%- if issue[:topics] -%>
    <topics>
      <%- issue[:topics].each do |topic| -%>
      <topic><%= topic %></topic>
      <%- end -%>
    </topics>
    <%- end -%>
  </issue>
  <%- end -%>
</issues>