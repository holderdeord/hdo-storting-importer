<?xml version="1.0" encoding="UTF-8"?>
<representatives>
  <%- representatives.each do |rep| -%>
  <representative>
    <externalId><%= rep.external_id %></externalId>
    <firstName><%= rep.first_name %></firstName>
    <lastName><%= rep.last_name %></lastName>
    <dateOfBirth><%= rep.date_of_birth %></dateOfBirth>
    <dateOfDeath><%= rep.date_of_death %></dateOfDeath>
    <district><%= rep.district %></district>
    <party><%= rep.party %></party>
    <committees>
      <%- rep.committees.each do |committee| -%>
      <committee><%= committee %></committee>
      <%- end -%>
    </committees>
    <period><%= rep.period %></period>
  </representative>
  <%- end -%>
</representatives>