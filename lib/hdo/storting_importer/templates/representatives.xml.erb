<?xml version="1.0" encoding="UTF-8"?>
<representatives>
  <%- representatives.each do |rep| -%>
  <representative>
    <externalId><%= rep[:externalId] %></externalId>
    <firstName><%= rep[:firstName] %></firstName>
    <lastName><%= rep[:lastName] %></lastName>
    <dateOfBirth><%= rep[:dateOfBirth] %></dateOfBirth>
    <dateOfDeath><%= rep[:dateOfDeath] %></dateOfDeath>
    <district><%= rep[:district] %></district>
    <party><%= rep[:party] %></party>
    <committees>
      <%- rep[:committees].each do |committee| -%>
      <committee><%= committee %></committee>
      <%- end -%>
    </committees>
    <period><%= rep[:period] %></period>
  </representative>
  <%- end -%>
</representatives>