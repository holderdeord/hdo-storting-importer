<?xml version="1.0" encoding="UTF-8"?>
<topics>
  <%- topics.each do |topic| -%>
  <topic>
    <externalId><%= topic[:externalId] %></externalId>
    <name><%= topic[:name] %></name>
    <subtopics>
      <%- topic[:subTopics].each do |subtopic| -%>
      <topic>
        <externalId><%= subtopic[:externalId] %></externalId>
        <name><%= subtopic[:name] %></name>
      </topic>
      <%- end -%>
    </subtopics>
  </topic>
  <%- end -%>
</topics>